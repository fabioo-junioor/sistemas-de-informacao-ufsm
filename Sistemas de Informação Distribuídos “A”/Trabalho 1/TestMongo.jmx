        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="MongoUpdate" enabled="true">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import com.mongodb.client.MongoCollection;
import static com.mongodb.client.model.Filters.*;
import static com.mongodb.client.model.Updates.*;


import org.bson.Document;
import org.bson.types.ObjectId;

int valor = 1100;

try {
	MongoCollection&lt;Document&gt; collection = vars.getObject(&quot;collection&quot;);
	
	collection.updateMany(eq(&quot;Ano de exibição&quot;, 2019), combine(set(&quot;Público no ano de exibição&quot;, valor)));
	collection.updateMany(eq(&quot;Ano de exibição&quot;, 2018), combine(set(&quot;Público no ano de exibição&quot;, valor)));
	collection.updateMany(eq(&quot;Ano de exibição&quot;, 2017), combine(set(&quot;Público no ano de exibição&quot;, valor)));
	collection.updateMany(eq(&quot;Ano de exibição&quot;, 2016), combine(set(&quot;Público no ano de exibição&quot;, valor)));
	collection.updateMany(eq(&quot;Ano de exibição&quot;, 2015), combine(set(&quot;Público no ano de exibição&quot;, valor)));
	collection.updateMany(eq(&quot;Ano de exibição&quot;, 2014), combine(set(&quot;Público no ano de exibição&quot;, valor)));
	collection.updateMany(eq(&quot;Ano de exibição&quot;, 2013), combine(set(&quot;Público no ano de exibição&quot;, valor)));
	collection.updateMany(eq(&quot;Ano de exibição&quot;, 2012), combine(set(&quot;Público no ano de exibição&quot;, valor)));
	collection.updateMany(eq(&quot;Ano de exibição&quot;, 2011), combine(set(&quot;Público no ano de exibição&quot;, valor)));
	collection.updateMany(eq(&quot;Ano de exibição&quot;, 2010), combine(set(&quot;Público no ano de exibição&quot;, valor)));
	collection.updateMany(eq(&quot;Ano de exibição&quot;, 2009), combine(set(&quot;Público no ano de exibição&quot;, valor)));

	return &quot;Document with id=&quot; + vars.get(&quot;exampleDocumentId&quot;) + &quot; modified&quot;;
}
catch (Exception e) {
	SampleResult.setSuccessful(false);
	SampleResult.setResponseCode(&quot;500&quot;);
	SampleResult.setResponseMessage(&quot;Exception: &quot; + e);
}</stringProp>
